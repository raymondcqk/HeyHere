<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.raymondqk.raysqlitepractice">

    <uses-permission android:name="android.permission.READ_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_CONTACTS"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.MOUNT_UNMOUNT_FILESYSTEMS"/>
    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE"/>

    <application
        android:allowBackup="true"
        android:icon="@drawable/ultra_ball_403px"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.LaunchActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"></action>
                <category android:name="android.intent.category.LAUNCHER"></category>
            </intent-filter>
        </activity>
        <activity android:name=".activity.GuideActivity"></activity>
        <activity android:name=".activity.SetPubInfoActivity"></activity>
        <activity android:name=".activity.MainActivity"></activity>
        <activity android:name=".activity.PubinfoChangeActivity"></activity>
        <activity android:name=".activity.ProjectActivity"></activity>
        <activity android:name=".activity.LoginActivity"></activity>
        <activity android:name=".activity.RegisterActivity"></activity>
        <activity android:name=".activity.ContactActivity"></activity>
        <activity android:name=".activity.WeatherActivity"></activity>
        <activity android:name=".activity.SelectCityActivity"></activity>

        <provider
            android:name="com.raymondqk.raysqlitepractice.utils.provider.WeatherProvider"
            android:authorities="com.raymondcqk.ishere.provider"></provider>
    </application>


</manifest>
